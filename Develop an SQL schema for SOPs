-- Table: public.SOPs

-- DROP TABLE public."SOPs";

CREATE TABLE public."SOPs"
(
    sop_title text COLLATE pg_catalog."default" NOT NULL,
    sop_authors text COLLATE pg_catalog."default" NOT NULL,
    sop_version text COLLATE pg_catalog."default" NOT NULL,
    sop_description text COLLATE pg_catalog."default" NOT NULL,
    sop_purpose text COLLATE pg_catalog."default" NOT NULL,
    sop_history_of_changes text COLLATE pg_catalog."default" NOT NULL,
    sop_scope text COLLATE pg_catalog."default" NOT NULL,
    sop_safety text COLLATE pg_catalog."default" NOT NULL,
    sop_equipment_and_labware text COLLATE pg_catalog."default" NOT NULL,
    sop_reagents text COLLATE pg_catalog."default" NOT NULL,
    sop_procedure text COLLATE pg_catalog."default" NOT NULL,
    sop_referencs text COLLATE pg_catalog."default" NOT NULL,
    sop_appendix text COLLATE pg_catalog."default" NOT NULL,
    sop_timeline text COLLATE pg_catalog."default" NOT NULL,
    sop_deviations_from_procedure text COLLATE pg_catalog."default" NOT NULL,
    "sop_ID" integer NOT NULL DEFAULT nextval('"SOPs_sop_ID_seq"'::regclass),
    CONSTRAINT "SOPs_pkey" PRIMARY KEY ("sop_ID")
)

TABLESPACE pg_default;

ALTER TABLE public."SOPs"
    OWNER to postgres;


-- Table: public.MDVFragments_from_excel

-- DROP TABLE public."MDVFragments_from_excel";

CREATE TABLE public."MDVFragments_from_excel"
(
    met_id text COLLATE pg_catalog."default" NOT NULL,
    formula text COLLATE pg_catalog."default" NOT NULL,
    elements text[] COLLATE pg_catalog."default" NOT NULL,
    positions integer[] NOT NULL,
    id integer NOT NULL DEFAULT nextval('"MDVFragments_id_seq"'::regclass),
    CONSTRAINT "MDVFragments_from_excel_pkey" PRIMARY KEY (met_id)
)

TABLESPACE pg_default;

ALTER TABLE public."MDVFragments_from_excel"
    OWNER to postgres;

-- Table: public.MFA model metabolites 

-- DROP TABLE public."MFA model metabolites ";

CREATE TABLE public."MFA model metabolites "
(
    id integer NOT NULL DEFAULT nextval('"MFA model metabolites _id_seq"'::regclass),
    mapping_id text COLLATE pg_catalog."default" NOT NULL,
    met_id text COLLATE pg_catalog."default" NOT NULL,
    met_elements text[] COLLATE pg_catalog."default",
    met_atompositions integer[],
    met_symmetry_elements text[] COLLATE pg_catalog."default",
    met_symmetry_atompositions integer[],
    used boolean,
    comment text COLLATE pg_catalog."default",
    met_mapping json,
    base_met_ids text[] COLLATE pg_catalog."default",
    base_met_elements json,
    base_met_atompositions json,
    base_met_symmetry_elements json,
    base_met_symmetry_atompositions json,
    base_met_indices integer[]
)

TABLESPACE pg_default;

ALTER TABLE public."MFA model metabolites "
    OWNER to postgres;

-- Table: public.MFA model reactions table

-- DROP TABLE public."MFA model reactions table";

CREATE TABLE public."MFA model reactions table"
(
    id integer NOT NULL DEFAULT nextval('"MFA model reactions table_id_seq"'::regclass),
    model_id text COLLATE pg_catalog."default" NOT NULL,
    rxn_id text COLLATE pg_catalog."default" NOT NULL,
    rxn_name text COLLATE pg_catalog."default",
    equation text COLLATE pg_catalog."default",
    subsystem text COLLATE pg_catalog."default",
    gpr text COLLATE pg_catalog."default",
    genes text[] COLLATE pg_catalog."default",
    reactants_stoichiometry double precision[],
    products_stoichiometry double precision[],
    reactants_ids text[] COLLATE pg_catalog."default",
    products_ids text[] COLLATE pg_catalog."default",
    lower_bound double precision,
    upper_bound double precision,
    objective_coefficient double precision,
    flux_units text COLLATE pg_catalog."default",
    fixed boolean,
    free boolean,
    reversibility boolean,
    weight double precision,
    used boolean,
    comment text COLLATE pg_catalog."default"
)

TABLESPACE pg_default;

ALTER TABLE public."MFA model reactions table"
    OWNER to postgres;

-- Table: public.measuredFluxes

-- DROP TABLE public."measuredFluxes";

CREATE TABLE public."measuredFluxes"
(
    id integer NOT NULL DEFAULT nextval('"measuredFluxes_id_seq"'::regclass),
    experiment_id text COLLATE pg_catalog."default" NOT NULL,
    model_id text COLLATE pg_catalog."default" NOT NULL,
    sample_name_abbreviation text COLLATE pg_catalog."default" NOT NULL,
    rxn_id text COLLATE pg_catalog."default" NOT NULL,
    flux_average double precision,
    flux_stdev double precision,
    flux_lb double precision,
    flux_ub double precision,
    flux_units text COLLATE pg_catalog."default",
    used boolean,
    comment text COLLATE pg_catalog."default"
)

TABLESPACE pg_default;

ALTER TABLE public."measuredFluxes"
    OWNER to postgres;

