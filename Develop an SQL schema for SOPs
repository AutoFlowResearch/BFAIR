-- Table: public.tracers

-- DROP TABLE public.tracers;

CREATE TABLE public.tracers
(
    met_id text COLLATE pg_catalog."default" NOT NULL,
    met_name text COLLATE pg_catalog."default",
    isotopomer_formula text[] COLLATE pg_catalog."default",
    met_elements text[] COLLATE pg_catalog."default",
    met_atompositions integer[],
    ratio double precision,
    supplier text COLLATE pg_catalog."default",
    supplier_reference text COLLATE pg_catalog."default",
    purity double precision,
    comment text COLLATE pg_catalog."default",
    id integer NOT NULL DEFAULT nextval('tracers_id_seq'::regclass)
)

TABLESPACE pg_default;

ALTER TABLE public.tracers
    OWNER to postgres;
